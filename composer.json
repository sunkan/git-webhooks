{
    "name": "sunkan/git-webhooks",
    "description": "normalise webhook events for github, gitlab and bitbucket",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "nyholm/psr7": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "DavidBadura\\GitWebhooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavidBadura\\GitWebhooks\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
